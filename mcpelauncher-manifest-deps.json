{
    "name": "mcpelauncher-manifest-deps",
    "buildsystem": "simple",
    "build-commands": [],
    "modules": [
        "pydeps.json",
        {
            "name": "libtinfo-clang9.0.0-release",
            "sources": [
                {
                    "type": "git",
                    "url": "git://ncurses.scripts.mit.edu/ncurses.git",
                    "tag": "v6.2",
                    "commit": "47d2fb4537d9ad5bb14f4810561a327930ca4280"
                }
            ],
            "config-opts": [ "--with-shared", "--with-normal", "--without-debug", "--without-ada", "--enable-widec", "--disable-pc-files", "--with-cxx-binding", "--with-cxx-shared", "--with-versioned-syms", "--with-abi-version=5" ],
            "post-install": [
                "ln -s /app/lib/libncursesw.so.5 /app/lib/libtinfo.so.5"
            ],
            "only-arches": [ "x86_64" ]
        }
    ]
}
